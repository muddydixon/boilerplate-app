{
  "name": "boilerplate-app",
  "version": "1.0.0",
  "description": "App Boilerplate including express / bookshelf / react / flux",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "watch": "DEBUG=boilerplate:* node-dev index.js",
    "jsx:build": "browserify -o public/js/bundle.js -t [ babelify --presets [ es2015 react ] ] jsx/bundle.js",
    "jsx:watch": "watchify -d -v -o public/js/bundle.js -t [ babelify --presets [ es2015 react ] ] jsx/bundle.js",
    "db:latest": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:reset": "npm run db:rollback && npm run db:latest"
  },
  "keywords": [
    "boilerplate",
    "app",
    "react",
    "express"
  ],
  "repository": "https://github.com/muddydixon/boilerplate-app.git",
  "author": "muddydixon <muddydixon@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.1",
    "bootstrap": "^3.3.7",
    "browserify": "^13.1.0",
    "checkit": "^0.7.0",
    "commander": "^2.9.0",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "flux": "^2.1.1",
    "font-awesome": "^4.6.3",
    "js-yaml": "^3.6.1",
    "knex": "^0.12.1",
    "mysql": "^2.11.1",
    "node-dev": "^3.1.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "serve-static": "^1.11.1",
    "uuid": "^2.0.3",
    "watchify": "^3.7.0"
  }
}
